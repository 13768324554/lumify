{
    "apiVersion": "0.4.0",
    "swaggerVersion": "1.2",
    "basePath": "http://localhost:8889",
    "resourcePath": "/graph",
    "apis": [
        {
            "path": "/graph/vertex/search",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Searches the graph for vertices",
                    "type": "GraphVertexSearchResult",
                    "nickname": "vertexSearch",
                    "parameters": [
                        {
                            "name": "q",
                            "description": "the phrase to search for",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        },
                        {
                            "name": "filter",
                            "description": "filter to apply",
                            "required": true,
                            "type": "string",
                            "paramType": "query"
                        },
                        {
                            "name": "offset",
                            "description": "offset from start of search results",
                            "required": false,
                            "type": "integer",
                            "format": "int32",
                            "paramType": "query"
                        },
                        {
                            "name": "size",
                            "description": "number of items to return",
                            "required": false,
                            "type": "integer",
                            "format": "int32",
                            "paramType": "query"
                        },
                        {
                            "name": "conceptType",
                            "description": "conceptType IRI to search for",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        },
                        {
                            "name": "leafNodes",
                            "description": "include child concept types",
                            "required": false,
                            "type": "boolean",
                            "paramType": "query"
                        },
                        {
                            "name": "relatedToVertexId",
                            "description": "returns vertices related to this vertex",
                            "required": false,
                            "type": "string",
                            "paramType": "query"
                        }
                    ]
                }
            ]
        }
    ],
    "models": {
        "GraphVertexSearchResult": {
            "id": "GraphVertexSearchResult",
            "properties": {
                "vertices": {
                    "type": "array",
                    "items": {
                        "type": "GraphVertexSearchVertex"
                    }
                },
                "nextOffset": {
                    "type": "integer",
                    "format": "int32"
                },
                "retrievalTime": {
                    "type": "integer",
                    "format": "int32"
                },
                "totalTime": {
                    "type": "integer",
                    "format": "int32"
                },
                "totalHits": {
                    "type": "integer",
                    "format": "int32"
                },
                "searchTime": {
                    "type": "integer",
                    "format": "int32"
                }
            }
        },
        "GraphVertexSearchVertex": {
            "id": "GraphVertexSearchVertex",
            "properties": {
                "id": {
                    "type": "string"
                },
                "sandboxStatus": {
                    "type": "string"
                },
                "edgeLabels": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "properties": {
                    "type": "array",
                    "items": {
                        "type": "LinkedHashMap"
                    }
                }
            }
        }
    }
}
